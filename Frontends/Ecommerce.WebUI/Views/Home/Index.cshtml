@model List<Ecommerce.DtoLayer.CatalogDtos.FeatureSliderDto.ResultFeatureSliderDto>

@{
    ViewData["Title"] = "Index";

}

<h1>Index</h1>

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <h4>Basic Tables</h4>
                <p>For basic styling&mdash;light padding and only horizontal dividers&mdash;add the base class <code>.table</code> to any <code>&lt;table&gt;</code>. It may seem super redundant, but given the widespread use of tables for other plugins like calendars and date pickers, we've opted to isolate our custom table styles.</p>
                <div class="bs-example" data-example-id="simple-table">
                    <table class="table">
                        <caption>
                            Optional table caption.
                        </caption>
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Gorsel Basligi</th>
                                <th>Sil</th>
                                <th>Guncelle</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                    <a href="/Admin/FeatureSlider/CreateFeatureSlider" class="btn btn-primary"> One Cikarilan Yeni Slider Gorsel girisi </a>
                </div>
            </div>
        </div>
    </div>
</section>